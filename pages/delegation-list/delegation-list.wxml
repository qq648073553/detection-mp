<!--pages/delegation-list/delegation-list.wxml-->
<import src="/template/deg-list/index.wxml" />

<view class="container" style="padding-top:{{navHeight}}px">
    <navbar page-name="委托列表" showNav showHome></navbar>
    <van-sticky>
        <van-search
                class="custom-class"
                value="{{searchValue}}"
                shape="round"
                background="#fff"
                placeholder="请输入搜索关键词"
        />
        <view wx:if="{{histories.length > 0}}"  class="index-histories">
            <view wx:for="{{histories}}" class="history-item">{{item}}</view>
        </view>
    </van-sticky>

    <view class="search-content">
        <template  is="degList" data="{{list:list}}"></template>
    </view>

</view>

