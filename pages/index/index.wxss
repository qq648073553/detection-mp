}
.index-header {
    display: flex;
    justify-content: space-between;
    background: #fff;
    padding: 20rpx 30rpx;
    border-bottom: 3px solid #F5F5F5;
}
.personal-center {
    color:  #4387F6;
    font-size: 30px;
}
.new-pro-btn  {
    min-width: 60px;
    height: 30px;
    line-height: 30px;
    padding: 0 8px;
    font-size: 12px;
    color: #4387F6;
    border: 1px solid #4387F6;
}

.index-pic-big {
    margin-top: 20rpx;
    width: 100vw;
    height: 120px;
}
.index-content{
    padding: 20rpx;
    background: #fff;
    border-radius: 5px;
}

.custom-class .van-search__content{
    border: 1px solid #4387F6;
    background: #fff;
}
.index-search .van-cell {
    border: 1px solid #4387F6;
    background: #fff;
    border-radius: 20px;
    width: 90%;
    margin: auto;
    border-radius: 20px;
    padding: 6px 16px;
}
.index-search .van-cell__title {
    color: #C8C9CC;
}
.index-histories {
    width: 90%;
    padding-left: 10px;
    margin: 10px auto;
    display: flex;
    overflow: hidden;
    flex-flow: row nowrap;
}
.index-histories .history-item{
    flex-shrink: 0;
    font-size: 12px;
    color: #000;
    border-radius: 5px;
    background: #F5F5F5;
    padding: 5px;
    margin-right: 10px;
}
.pro-header {
    width: 100%;
    position: fixed;
    height: 100px;
    overflow: hidden;
    /*top: 60px;*/
    z-index: 999;
    background: #fff;
    transition: all .5s;
}
