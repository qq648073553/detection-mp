<view class="container">
    <image src="../../assets/images/index.jpg" class="index-pic-big"></image>
    <van-notice-bar
            background="#fff"
            left-icon="volume-o"
            text="在代码阅读过程中人们说脏话的频率是衡量代码质量的唯一标准。"
    />
    <view class="index-content">
        <view class="index-module">
            <van-cell title="检测业务" border="{{false}}" is-link value="查看更多" url="/pages/function-list/function-list" />
            <van-row gutter="20">
                <van-col span="12">
                    <view class="fd-card" bindtap="goPage" url="/pages/check-in/check-in">
                        <icon class="iconfont icon-dengji card-icon"></icon>
                        <view class="card-title">信息登记</view>
                    </view>
                </van-col>
                <van-col span="12">
                    <view class="fd-card">
