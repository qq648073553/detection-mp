<!--pages/pro-search/pro-search.wxml-->
<import src="/template/pro-list/index.wxml" />
<van-sticky>
    <van-search
            class="custom-class"
            value="{{searchValue}}"
            shape="round"
            background="#fff"
            placeholder="请输入搜索关键词"
    />
</van-sticky>

<view class="search-content">
    <template is="proList" data="{{list:list}}"></template>
</view>
